int main () {
    int i,n;
    printf("Enter the number: \n");
    scanf("%d", &n);

    if (n<0) {
        printf("No factor for negative number.");
    }else {
        printf("factor of %d: ", n);
        for (i = 1; i <= n; i++) {
            if (n % i == 0) {
                printf("%d", i);
            }
        }
        printf("\n");
    }
    return 0;
}
