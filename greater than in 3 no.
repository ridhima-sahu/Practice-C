int main() {
    int x,y,z;
    printf("enter the number x :\n ");
    scanf("%d",&x);
    printf("enter the number y:\n ");
    scanf("%d",&y);
    printf("enter the number z:\n ");
    scanf("%d",&z);
    if (x>=y && x>=z) {
        printf("x is greatest number\n",x);
    }
    else if (y>=x && y>=z) {
        printf("y is greatest number\n",y);
    }
    else (z>=x && z>=y) {
        printf("z is greatest number\n",z);
    }

    return 0;
}
