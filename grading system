int main() {
    int marks;
    printf("enter the number: \n");
    scanf("%d",&marks);
    if (marks >= 80 && marks <= 100) {
        printf("A");
    }
    else if (marks < 80 && marks >= 70) {
        printf("B");
    }
    else if (marks < 70 && marks >= 60) {
        printf("C");
    }
    else if (marks < 60 && marks >= 40) {
        printf("D");
    }
    else if (marks < 40) {
        printf("Fail");
    }
        return 0;
}
